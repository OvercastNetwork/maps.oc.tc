    </kit>
    <kit name="yellow" parents="teams">
        <item slot="3" amount="24" damage="4">stained clay</item>
        <item slot="4" amount="4" damage="4">stained glass</item>
        <helmet locked="true" unbreakable="true" color="E5E533">leather helmet</helmet>
