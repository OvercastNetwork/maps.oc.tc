            <cuboid min="-3,8,-85" max="4,10,-80"/>
        </region>
    </net>
    <net points="1" capture-filter="only-yellow" flags="blue-flag">
        <region>
            <cuboid min="-3,8,-85" max="4,10,-80"/>
        </region>
    </net>
    <net points="1" capture-filter="only-yellow" flags="green-flag">
        <region>
            <cuboid min="-3,8,-85" max="4,10,-80"/>
        </region>
    </net>

    <net points="1" capture-filter="only-blue" flags="red-flag">
