<itemkeep>
    <item>golden apple</item>
</itemkeep>
<tnt>
    <power>3</power>
    <fuse>1s</fuse>
    <instantignite>true</instantignite>
    <licensing>off</licensing>
    <friendly-defuse>off</friendly-defuse>
</tnt>
<renewables>
    <renewable rate="30" grow="true" avoid-entities="true" particles="true" sound="true">
        <region>
            <everywhere/>
