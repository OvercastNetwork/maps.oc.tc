        <region>
            <cuboid min="-140,0,140" max="155,25,0"/>
        </region>
        <renew-filter>
            <not>
                <block>stained glass</block>
                <block>air</block>
            </not>
        </renew-filter>
        <replace-filter>
            <any>
                <block>air</block>
                <block>water</block>
                <block>stationary water</block>
            </any>
        </replace-filter>
    </renewable>
    <renewable rate="1" grow="false" avoid-entities="true" particles="false" sound="false">
        <region>
            <region name="points"/>
        </region>
        <renew-filter>
            <not>
                <block>stained glass</block>
                <block>air</block>
            </not>
        </renew-filter>
        <replace-filter>
            <any>
                <block>air</block>
                <block>water</block>
