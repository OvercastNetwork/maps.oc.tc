        <regions yaw="135">
            <cylinder base="95,54,255" radius="2" height="0"/>
        </regions>
    </default>
    <spawns team="lime-team" kit="lime">
        <spawn yaw="90">
            <region>
                <cuboid min="56.5,11.5,247.5" max="55.5,12,246.5"/>
