            </region>
        </spawn>
    </spawns>
    <spawns team="yellow-team" kit="yellow">
        <spawn yaw="180">
            <region>
                <cuboid min="-118.5,11.5,216.5" max="-117.5,12,215.5"/>
            </region>
        </spawn>
        <spawn yaw="0">
            <region>
                <cuboid min="-117.5,11.5,72.5" max="-118.5,12,73.5"/>
            </region>
        </spawn>
    </spawns>
    <spawns team="magenta-team" kit="magenta">
