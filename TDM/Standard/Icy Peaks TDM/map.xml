</score>
<teams>
    <team color="dark red" max="20">Red</team>
    <team color="blue" max="20">Blue</team>
</teams>
<kits>
    <kit name="spawn">
        <item slot="0">stone sword</item>
        <item slot="1">bow</item>
        <item slot="2" amount="12">arrow</item>
        <item slot="8" amount="32">bread</item>
        <chestplate enchantment="protection projectile:2">iron chestplate</chestplate>  
        <potion duration="5" amplifier="3">heal</potion>
        <potion duration="5" amplifier="3">damage resistance</potion>
        <item slot="9" amount="32" damage="4">ink sack</item>
    </kit>
        <kit name="red" parents="spawn">
        <helmet color="993333" unbreakable="true">leather helmet</helmet>
        <boots color="993333" unbreakable="true">leather boots</boots>
    </kit>
        <kit name="blue" parents="spawn">
        <helmet color="334CB2" unbreakable="true">leather helmet</helmet>
        <boots color="334CB2" unbreakable="true">leather boots</boots>
    </kit>
</kits>
<regions>
    <apply block="deny-all">
        <rectangle min="-oo,-oo" max="oo,oo"/>
    </apply>
</regions>
<itemremove>
    <item>bread</item>
    <item>leather helmet</item>
    <item>iron chestplate</item>
    <item>leather boots</item>
    <item>arrow</item>
</itemremove>
<toolrepair>
    <tool>stone sword</tool>
    <tool>bow</tool>
</toolrepair>
<killreward>
    <item amount="6">arrow</item>
    <item amount="1">exp bottle</item>
</killreward>
<spawns>
    <spawn team="red" kit="red" yaw="-135">
        <block>-51.5,10,-6.5</block>
    </spawn>
    <spawn team="blue" kit="blue" yaw="135">
        <block>158.5,10,-6.5</block>
    </spawn>
    <default yaw="-90">
        <block>52,43.2,32</block>
    </default>
</spawns>
