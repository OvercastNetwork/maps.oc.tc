<?xml version="1.0"?>
<map proto="1.3.6">
<name>Terminal Frost</name>
<version>1.0</version>
<objective>Have the most kills before time runs out!</objective>
<authors>
    <author uuid="d341db34-f1ab-406e-b290-da6a6703565e"/>
    <!--CheetoPuf-->
    <author uuid="b0b3c5d5-e9bd-4984-bc07-1bd552d4b042"/>
    <!--Beebou-->
</authors>
<teams>
    <team color="dark aqua" max="32" max-overfill="36">Cyan</team>
    <team color="gold" max="32" max-overfill="36">Orange</team>
</teams>
<kits>
    <kit name="spawn">
        <helmet color="ffffff" locked="true">leather helmet</helmet>
        <leggings color="ffffff" locked="true">leather leggings</leggings>
        <boots color="ffffff" locked="true">leather boots</boots>
        <item slot="0">stone sword</item>
        <item slot="1" enchantment="arrow infinite:4">bow</item>
        <item slot="2">arrow</item>
    </kit>
    <kit name="cyan" parents="spawn">
        <chestplate color="4c7f99" locked="true" enchantment="protection projectile:2">leather chestplate</chestplate>
        <item slot="8" damage="9">wool</item>
    </kit>
    <kit name="orange" parents="spawn">
        <chestplate color="d87f33" locked="true" enchantment="protection projectile:2">leather chestplate</chestplate>
        <item slot="8" damage="1">wool</item>
    </kit>
</kits>
<spawns>
    <spawn team="cyan" kit="cyan">
        <regions yaw="90">
            <cuboid min="86,10,5" max="91,10,10"/>
        </regions>
    </spawn>
    <spawn team="orange" kit="orange">
        <regions yaw="-90">
            <cuboid min="-86,10,5" max="-81,10,10"/>
        </regions>
    </spawn>
    <default>
        <regions yaw="0">
            <block>4.5,39,0.5</block>
            <block>0.5,39,0.5</block>
        </regions>
    </default>
</spawns>
<score>
    <time>10m</time>
    <kills>1</kills>
</score>
<regions>
    <apply block="deny-all">
        <rectangle min="-oo,-oo" max="oo,oo"/>
    </apply>
</regions>
<killreward>
    <item>golden apple</item>
</killreward>
<toolrepair>
    <tool>leather helmet</tool>
    <tool>leather chestplate</tool>
    <tool>leather leggings</tool>
    <tool>leather boots</tool>
    <tool>stone sword</tool>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>arrow</item>
    <item>golden apple</item>
    <item>wool</item>
</itemremove>
<hunger>
    <depletion>off</depletion>
</hunger>
</map>