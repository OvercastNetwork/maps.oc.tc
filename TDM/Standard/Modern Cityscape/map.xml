<?xml version="1.0"?>
<map proto="1.3.0">
<name>Modern Cityscape</name>
<version>1.2.9</version>
<objective>Be the team with the highest score after 10 minutes!</objective>
<authors>
    <author uuid="2a289d2a-d970-49c5-9a6c-01fc0264e317"/> <!-- Stealth5061 -->
</authors>
<contributors>
    <contributor uuid="1379cb6e-f291-4498-9807-e636f9674ac0" contibution="Suggestions and aesthetics help"/> <!-- SH4D0W_HAWK -->
</contributors>
<teams>
    <team color="green" max="50">Lime</team>
    <team color="dark aqua" max="50">Cyan</team>
</teams>
<filters>
    <filter name="allow-some" parents="deny-all">
        <allow>
            <block>ladder</block>
            <block>glass</block>
            <block>thin glass</block>
        </allow>
    </filter>
</filters>
<regions>
    <apply block="deny-all">
        <rectangle name="r-spawn1a" min="70,86" max="81,97"/>
        <rectangle name="r-spawn1b" min="-90,-74" max="-79,-63"/>
        <rectangle name="r-spawn2a" min="70,-74" max="81,-63"/>
        <rectangle name="r-spawn2b" min="-90,86" max="-79,97"/>
    </apply>
    <apply block="allow-some">
        <rectangle min="-oo,-oo" max="oo,oo"/>
    </apply>
</regions>
<kits>
    <kit name="spawn">
        <item slot="0" unbreakable="true">stone sword</item>
        <item slot="1" enchantment="arrow infinite:1" unbreakable="true">bow</item>
        <item slot="2" amount="32">cooked chicken</item>
        <item slot="3" amount="2">golden apple</item>
        <potion duration="5">heal</potion>
        <potion duration="10">damage resistance</potion>
        <potion duration="oo">speed</potion>
        <item slot="4" amount="16">ladder</item>
        <item slot="28" amount="1">arrow</item>
        <chestplate unbreakable="true">chainmail chestplate</chestplate>
    </kit>
    <kit name="lime" parents="spawn">
        <helmet color="2BE058" unbreakable="true">leather helmet</helmet>
        <leggings color="2BE058" unbreakable="true">leather leggings</leggings>
        <boots color="2BE058" unbreakable="true">leather boots</boots>
    </kit>
    <kit name="cyan" parents="spawn">
        <helmet color="3089A8" unbreakable="true">leather helmet</helmet>
        <leggings color="3089A8" unbreakable="true">leather leggings</leggings>
        <boots color="3089A8" unbreakable="true">leather boots</boots>
    </kit>
</kits>
<spawns>
    <spawn team="lime" kit="lime">
        <point yaw="180">
            <cuboid name="spawn1a" min="-87,15,-67" max="-85,15,-69"/>
        </point>
        <point yaw="90">
            <cuboid name="spawn1b" min="-85,15,-69" max="-83,15,-71"/>
        </point>
    </spawn>
    <spawn team="cyan" kit="cyan">
        <point yaw="90">
            <cuboid name="spawn2a" min="-85,15,92" max="-83,15,94"/>
        </point>
        <point yaw="0">
            <cuboid name="spawn2b" min="-87,15,90" max="-85,15,92"/>
        </point>
    </spawn>
    <default yaw="0">
        <cuboid min="-6,45,9" max="-2,45,15"/>
    </default>
</spawns>
<maxbuildheight>64</maxbuildheight>
<score>
    <time>10m</time>
</score>
<toolrepair>
    <tool>stone sword</tool>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>leather helmet</item>
    <item>leather leggings</item>
    <item>leather boots</item>
    <item>chainmail chestplate</item>
    <item>arrow</item>
    <item>cooked chicken</item>
    <item>clay</item>
    <item>brown mushroom</item>
    <item>red mushroom</item>
    <item>log</item>
    <item>wool</item>
    <item>grass</item>
    <item>fence</item>
    <item>workbench</item>
    <item>chest</item>
    <item>ladder</item>
</itemremove>
</map>

