<?xml version="1.0"?>
<map proto="1.3.2">
<name>101 Rooms TDM</name>
<version>1.0.2</version>
<objective>Get the most kills after 5 minutes!</objective>
<authors>
    <author uuid="06078e7e-6a7b-4d31-8f74-853072b182bb" contribution="Map Idea and Design"/> <!-- xXFracXx -->
</authors>
<contributors>
    <contributor uuid="b55d6988-42a6-43a1-ae08-f3f693d64d2e" contribution="Helped with Map Design"/> <!-- PhoenixMatt -->
    <contributor uuid="bf331953-4f92-43ee-8abc-7544b8234936" contribution="Helped with Map Design"/> <!-- ParaSpider -->
    <contributor uuid="82d03646-d1a1-4383-a533-9509b130f751" contribution="Idea for Rage w/ 2 Lives"/> <!-- iLifting -->
    <contributor uuid="22ba442b-cf26-4d09-b290-8d4e50a551b2" contribution="Improved XML"/> <!-- dinner1111 -->
    <contributor uuid="9bfbc560-244a-4e28-ab70-185f39740ec1" contribution="Found the Ender Pearl glitch"/> <!-- TheZaner -->
    <contributor uuid="10c7915f-26b2-4cd6-9d42-7675a9259374" contribution="Help with spawner"/> <!-- RabidRabbit30 -->
</contributors>
<teams>
    <team color="blue" max="6" max-overfill="8">Blue</team>
    <team color="green" max="6" max-overfill="8">Green</team>
    <team color="yellow" max="6" max-overfill="8">Yellow</team>
    <team color="dark red" max="6" max-overfill="8">Red</team>
</teams>
<rage/>
<score>
    <time>5m</time>
</score>
<spawns safe="true">
    <spawn team="blue" kit="blue" yaw="45">
        <cuboid min="-318,1,-335" max="-316,2,-333"/>
    </spawn>
    <spawn team="green" kit="green" yaw="135">
        <cuboid min="-318,1,-263" max="-316,2,-261"/>
    </spawn>
    <spawn team="yellow" kit="yellow" yaw="-45">
        <cuboid min="-390,1,-335" max="-388,2,-333"/>
    </spawn>
    <spawn team="red" kit="red" yaw="-135">
        <cuboid min="-390,1,-263" max="-388,2,-261"/>
    </spawn>
    <default yaw="90">
        <cylinder base="-293,25,-298" radius="3" height="0"/>
    </default>
</spawns>
<kits>
    <kit name="spawn">
        <item slot="0" enchantment="damage all:10" name="Knife">iron sword</item>
        <item slot="1" enchantment="arrow damage:10" name="Bow">bow</item>
        <item slot="3" amount="1" name="`4Landmine `6| `bFuse: 1s `6| `bPower: 2">tnt</item>
        <item slot="4" amount="1">arrow</item>
        <potion duration="4" amplifier="1">heal</potion>
        <potion duration="4" amplifier="10">damage resistance</potion>
        <potion duration="oo" amplifier="-4">health boost</potion>           
    </kit>
    <kit name="blue" parents="spawn">
        <helmet color="0066cc" name="Helmet">leather helmet</helmet>
        <chestplate color="0066cc" name="Kevlar">leather chestplate</chestplate>
    </kit>
    <kit name="green" parents="spawn">
        <helmet color="00cd00" name="Helmet">leather helmet</helmet>
        <chestplate color="00cd00" name="Kevlar">leather chestplate</chestplate>
    </kit>
    <kit name="yellow" parents="spawn">
        <helmet color="ffff00" name="Helmet">leather helmet</helmet>
        <chestplate color="ffff00" name="Kevlar">leather chestplate</chestplate>
    </kit>
    <kit name="red" parents="spawn">
        <helmet color="cd0000" name="Helmet">leather helmet</helmet>
        <chestplate color="cd0000" name="Kevlar">leather chestplate</chestplate>
    </kit>
</kits>
<regions>
    <rectangle name="playarea" min="-oo,-oo" max="oo,oo"/>
    <cuboid name="roof" min="-oo,5,-oo" max="oo,6,oo"/>
    <apply block-break="deny-all">
        <rectangle min="-oo,-oo" max="oo,oo"/>
    </apply>
</regions>
<playable>
    <complement>
        <region name="playarea"/>
        <region name="roof"/>
    </complement>
</playable>
<timelock>on</timelock>
<killreward>
    <item amount="1">arrow</item>
    <item name="`4Landmine `6| `bFuse: 1s `6| `bPower: 2">tnt</item>
</killreward>
<itemremove>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>arrow</item>
    <item>tnt</item>
</itemremove>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
</toolrepair>
<tnt>
    <instantignite>on</instantignite>
    <licensing>off</licensing>
    <friendly-defuse>off</friendly-defuse>
    <blockdamage>off</blockdamage>
    <fuse>1s</fuse>
    <power>2</power>
</tnt>
<disabledamage>
    <damage ally="true" self="true" enemy="false" other="false">block explosion</damage>
</disabledamage>
<modifybowprojectile>
    <potion duration="3" amplifier="1">blindness</potion>
</modifybowprojectile>
<hunger>
    <depletion>off</depletion>
</hunger>
<autorespawn/>
</map>
