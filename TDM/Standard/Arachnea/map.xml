<?xml version="1.0"?>
<map proto="1.3.0">
<name>Arachnea</name>
<version>1.0</version>
<objective>Get the most kills in 10 minutes!</objective>
<authors>
    <author uuid="2ca8072f-74be-4798-85b8-bbce03aa91af"/> <!-- Tywnis -->
</authors>
<contributors>
    <contributor uuid="205d1aad-c619-499b-9885-b91c372b1c13" contribution="XML"/> <!-- lord_creeper01 -->
</contributors>
<teams>
    <team color="blue" max="40">Blue</team>
    <team color="dark red" max="40">Red</team>
</teams>
<kits>
    <kit name="spawn">
        <item slot="0" unbreakable="true" name="Sting">iron sword</item>
        <item slot="1" unbreakable="true" enchantment="arrow infinite:1">bow</item>
        <item slot="28" amount="1">arrow</item>
        <item slot="2" name="Baked Spider Flesh" amount="32">baked potato</item>
        <item slot="3" amount="3">web</item>
        <item slot="4" name="Spider Blood" damage="16373">potion</item>
        <item slot="5" name="Spider Milk">milk bucket</item>
        <potion duration="2" amplifier="1">heal</potion>
        <potion duration="5">damage resistance</potion>
        <chestplate>iron chestplate</chestplate>
        <boots>iron boots</boots>
    </kit>
    <kit name="red" parents="spawn">
        <helmet color="993333" unbreakable="true">leather helmet</helmet>
        <leggings color="993333" unbreakable="true">leather leggings</leggings>
    </kit>
    <kit name="blue" parents="spawn">
        <helmet color="334CB2" unbreakable="true">leather helmet</helmet>
        <leggings color="334CB2" unbreakable="true">leather leggings</leggings>
    </kit>
</kits>
<spawns>
    <spawn team="blue" kit="blue" yaw="135">
        <region name="blue-spawn-room"/>
    </spawn>
    <spawn team="red" kit="red" yaw="45">
        <region name="red-spawn-room"/>
    </spawn>
    <default yaw="0">2060,106,-991</default>
</spawns>
<regions>
    <cuboid name="blue-spawn-room" min="2135,32,-987" max="2138,32,-984"/>
    <cuboid name="red-spawn-room" min="2052,32,-1070" max="2055,32,-1067"/>
    <block name="spider-portal">2095,7,-1027</block>
    <apply block="allow-webs">
        <rectangle min="-oo,-oo" max="oo,oo"/>
    </apply>
</regions>
<portals>
    <!--  portals from the spider pit -->
    <portal x="@2095" y="@13" z="@-1026" yaw="@90" pitch="@0">
        <region name="spider-portal"/>
    </portal>
</portals>
<score>
    <time>10m</time>
</score>
<filters>
    <filter name="allow-webs">
        <block>web</block>
    </filter>
</filters>
<toolrepair>
    <tool>bow</tool>
    <tool>iron sword</tool>
</toolrepair>
<itemremove>
    <item>arrow</item>
    <item>iron sword</item>
    <item>leather helmet</item>
    <item>iron chestplate</item>
    <item>leather leggings</item>
    <item>iron boots</item>
    <item>baked potato</item>
    <item>glass bottle</item>
    <item>milk bucket</item>
    <item>bucket</item>
    <item>string</item>
</itemremove>
<timelock>off</timelock>
<mobs>
    <filter>
        <spawn>spawner</spawn>
    </filter>
</mobs>
</map>
