<map proto="1.3.0">
<name>Zen</name>
<version>1.0.1</version>
<objective>Eliminate all opposing teammates to win the match.</objective>
<gamemode>rage</gamemode>
<authors>
    <author uuid="2a289d2a-d970-49c5-9a6c-01fc0264e317"/>
    <!--  Stealth5061  -->
    <author uuid="2ca8072f-74be-4798-85b8-bbce03aa91af"/>
    <!--  Tywnis  -->
    <author uuid="82c796a5-c033-43be-af30-fa06496995f9"/>
    <!--  IM_A_H0B0  -->
</authors>
<teams>
    <team color="dark red" max="16" max-overfill="18">Red</team>
    <team color="blue" max="16" max-overfill="18">Blue</team>
</teams>
<kits>
    <kit name="spawn">
        <item slot="0" enchantment="damage all:10" lore="`71-Hit Kill!">iron sword</item>
        <item slot="1" enchantment="arrow damage:10" lore="`71-Shot Kill!">bow</item>
        <item slot="2" amount="1">arrow</item>
        <chestplate>iron chestplate</chestplate>
        <leggings>chainmail leggings</leggings>
    </kit>
    <kit name="red" parents="spawn">
        <helmet color="C40000">leather helmet</helmet>
        <boots color="C40000">leather boots</boots>
    </kit>
    <kit name="blue" parents="spawn">
        <helmet color="000099">leather helmet</helmet>
        <boots color="000099">leather boots</boots>
    </kit>
</kits>
<hunger>
    <depletion>off</depletion>
</hunger>
<spawns>
    <default yaw="0">
        <cylinder base="11.5,18.1,14.5" radius="2" height="0"/>
    </default>
    <spawn team="red" yaw="-180" kit="red">
        <cuboid min="52,8.1,-5" max="57,8.1,-2"/>
    </spawn>
    <spawn team="blue" yaw="0" kit="blue">
        <cuboid min="-29,8.1,-58" max="-34,8.1,-61"/>
    </spawn>
</spawns>
<regions>
    <apply block="deny-all">
        <rectangle min="-oo,-oo" max="oo,oo"/>
    </apply>
</regions>
<blitz>
    <time>5m</time>
    <lives>1</lives>
    <title>Blitz: Rage</title>
</blitz>
<rage/>
<timelock>on</timelock>
<killreward>
    <item>arrow</item>
</killreward>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>leather helmet</item>
    <item>iron chestplate</item>
    <item>chainmail leggings</item>
    <item>leather boots</item>
</itemremove>
</map>
