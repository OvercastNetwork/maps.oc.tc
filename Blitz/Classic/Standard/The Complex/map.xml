<?xml version="1.0"?>
<map proto="1.3.0">
<name>The Complex</name>
<version>1.0.4</version>
<objective>Blitz: Eliminate the other team before 10 minutes is up</objective>
<gamemode>blitz</gamemode>
<authors>
