<?xml version="1.0"?>
<map proto="1.3.3">
<name>Wind Waker</name>
<version>1.0.5</version>
<objective>Blitz: Eliminate the other team within 10 minutes.</objective>
<game-mode>blitz</game-mode>
<authors>
    <author uuid="130be12a-f3b9-4b7d-ad39-7b45e84d3d0f"/> <!-- Veiyl -->
</authors>
<contributors>
    <contributor uuid="2ca8072f-74be-4798-85b8-bbce03aa91af" contribution="XML"/> <!-- Tywnis -->
    <contributor uuid="e5eea785-51c1-40fe-a2d0-f83d4c035cd9" contribution="XML Fixes"/> <!-- pikpikmerchant -->
</contributors>
<timelock>on</timelock>
<teams>
    <team color="dark red" max="8">Red</team>
    <team color="blue" max="8">Blue</team>
</teams>
<blitz>
    <time>10m</time>
    <lives>1</lives>
</blitz>
<kits>
    <kit name="teams">
        <item slot="0">stone sword</item>
        <item slot="1">bow</item>
        <item slot="2" amount="16">arrow</item>
        <item slot="3">golden apple</item>
        <leggings unbreakable="true">iron leggings</leggings>
    </kit>
    <kit name="red" parents="teams">
        <helmet color="cd0000" unbreakable="true">leather helmet</helmet>
        <chestplate color="cd0000" enchantment="protection projectile" unbreakable="true">leather chestplate</chestplate>
        <boots color="cd0000" unbreakable="true">leather boots</boots>
    </kit>
    <kit name="blue" parents="teams">
        <helmet color="0066cc" unbreakable="true">leather helmet</helmet>
        <chestplate color="0066cc" enchantment="protection projectile" unbreakable="true">leather chestplate</chestplate>
        <boots color="0066cc" unbreakable="true">leather boots</boots>
    </kit>
</kits>
<itemremove>
    <item>cooked beef</item>
    <item>arrow</item>
    <item>golden apple</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>iron leggings</item>
    <item>leather boots</item>
</itemremove>
<toolrepair>
    <tool>stone sword</tool>
    <tool>bow</tool>
</toolrepair>
<regions>
    <apply block="deny-all">
        <rectangle min="-oo,-oo" max="oo,oo"/>
    </apply>
</regions>
<spawns>
    <spawn team="red" kit="red" yaw="-135">
        <point>14.5,16,30.5</point>
    </spawn>
    <spawn team="blue" kit="blue" yaw="45">
        <point>-13.5,16,-29.5</point>
    </spawn>
    <default yaw="180">
        <point>0.5,6,0.5</point>
    </default>
</spawns>
<include src="blitz-global.xml"/>
<hunger>
    <depletion>off</depletion>
</hunger>
</map>
