<item-mods>
    <rule>
        <match>
            <material>skull item</material>
        </match>
        <modify name="`rExploding Gift" />
    </rule>
</item-mods>
<regions>
    <apply block="allowed-blocks" use="use" />
</regions>
<block-drops trample="true">
    <rule trample="true">
        <filter>
            <all>
                <cause>trample</cause>
                <class>Necromancer</class>
                <any>
                    <all>
                        <sprinting />
                        <random>0.6</random>
                    </all>
                    <all>
                        <walking />
                        <random>0.4</random>
                    </all>
                    <all>
                        <crouching />
                        <random>0.2</random>
                    </all>
                </any>
                <any>
                    <material>grass</material>
                    <material>smooth brick</material>
                    <material>gravel</material>
                    <material>stone</material>
                    <material>double step</material>
                    <material>wood</material>
                    <material>stained clay</material>
                    <material>cobblestone</material>
                </any>
            </all>
        </filter>
        <replacement>redstone block</replacement>
    </rule>
    <rule trample="true" kit="witch-effects">
        <filter>
            <all>
                <cause>trample</cause>
                <material>redstone block</material>
                <not>
                    <class>Necromancer</class>
                </not>
            </all>
        </filter>
    </rule>
</block-drops>
<renewables>
    <renewable interval="6s" grow="true" particles="true">
        <renew-filer>
            <any>
                <material>grass</material>
                <material>smooth brick</material>
                <material>gravel</material>
                <material>stone</material>
                <material>double step</material>
                <material>wood</material>
                <material>stained clay</material>
                <material>cobblestone</material>
            </any>
        </renew-filer>
        <replace-filter>
            <material>redstone block</material>
        </replace-filter>
    </renewable>
</renewables>
